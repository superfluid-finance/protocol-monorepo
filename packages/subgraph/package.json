{
    "name": "superfluid-subgraph",
    "private": true,
    "version": "0.0.1",
    "description": "Subgraph for the Superfluid Ethereum contracts",
    "homepage": "https://github.com/superfluid-finance/protocol-monorepo/tree/dev/packages/subgraph",
    "repository": "github:superfluid-finance/protocol-monorepo",
    "license": "MIT",
    "scripts": {
        "getAbi": "node scripts/getAbi",
        "codegen": "graph codegen",
        "build": "graph build",
        "test": "truffle test --network ganache",
        "prepare-local": "mustache config/ganache.json subgraph.template.yaml > subgraph.yaml",
        "create-local": "graph create superfluid-test --node http://localhost:8020/",
        "remove-local": "graph remove superfluid-test --node http://localhost:8020/",
        "deploy-local": "graph deploy superfluid-test --node http://localhost:8020/ --ipfs http://localhost:5001",
        "watch": "graph deploy superfluid-test --node http://localhost:8020/ --ipfs http://localhost:5001 --watch",
        "deploy-all": "run-s deploy:*",
        "deploy": "graph deploy $SUBGRAPH_NAME --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs",
        "deploy:master": "yarn deploy-all",
        "deploy:feature": "yarn prepare:feature && SUBGRAPH_NAME=superfluid-finance/superfluid-feature yarn deploy",
        "deploy:dev": "yarn prepare:dev && SUBGRAPH_NAME=superfluid-finance/superfluid-dev yarn deploy",
        "deploy:xdai": "yarn prepare:xdai && SUBGRAPH_NAME=superfluid-finance/superfluid-xdai yarn deploy",
        "deploy:matic": "yarn prepare:matic && SUBGRAPH_NAME=superfluid-finance/superfluid-matic yarn deploy",
        "deploy:goerli": "yarn prepare:goerli && SUBGRAPH_NAME=superfluid-finance/superfluid-goerli yarn deploy",
        "deploy:rinkeby": "yarn prepare:rinkeby && SUBGRAPH_NAME=superfluid-finance/superfluid-rinkeby yarn deploy",
        "deploy:ropsten": "yarn prepare:ropsten && SUBGRAPH_NAME=superfluid-finance/superfluid-ropsten yarn deploy",
        "deploy:kovan": "yarn prepare:kovan && SUBGRAPH_NAME=superfluid-finance/superfluid-kovan yarn deploy",
        "deploy:mumbai": "yarn prepare:mumbai && SUBGRAPH_NAME=superfluid-finance/superfluid-mumbai yarn deploy",
        "prepare:feature": "mustache config/rinkeby.json subgraph.template.yaml > subgraph.yaml",
        "prepare:dev": "mustache config/rinkeby.json subgraph.template.yaml > subgraph.yaml",
        "prepare:xdai": "mustache config/xdai.json subgraph.template.yaml > subgraph.yaml",
        "prepare:matic": "mustache config/matic.json subgraph.template.yaml > subgraph.yaml",
        "prepare:goerli": "mustache config/goerli.json subgraph.template.yaml > subgraph.yaml",
        "prepare:rinkeby": "mustache config/rinkeby.json subgraph.template.yaml > subgraph.yaml",
        "prepare:ropsten": "mustache config/ropsten.json subgraph.template.yaml > subgraph.yaml",
        "prepare:kovan": "mustache config/kovan.json subgraph.template.yaml > subgraph.yaml",
        "prepare:mumbai": "mustache config/mumbai.json subgraph.template.yaml > subgraph.yaml",
        "lint": "run-s lint:*",
        "lint:js-prettier": "prettier --check  **/*.js && echo '✔  Your .js files look pretty.'",
        "lint:js-eslint": "eslint . --max-warnings=0 --report-unused-disable-directives --fix && echo '✔  Your .js files look good.'",
        "pre-commit": "true"
    },
    "dependencies": {
        "@decentral.ee/web3-helpers": "^0.5.3",
        "@graphprotocol/graph-cli": "0.18.0",
        "@graphprotocol/graph-ts": "0.18.0",
        "@superfluid-finance/js-sdk": "0.5.2",
        "ganache-time-traveler": "^1.0.15",
        "mustache": "^4.0.1",
        "truffle": "^5.2.3"
    },
    "workspaces": {
        "nohoist": [
            "**/@graphprotocol",
            "**/@graphprotocol/**"
        ]
    }
}
